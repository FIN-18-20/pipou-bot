{
  "name": "pipou-bot",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "check-types": "tsc --noEmit",
    "heroku-postbuild": "npm run build",
    "format": "prettier --write src tests",
    "lint": "eslint src --ext ts",
    "lint-fix": "npm run lint -- --fix",
    "start": "nodemon --watch \"src/**/*.ts\" -e ts --exec \"ts-node src/bot.ts | pino-pretty\"",
    "test": "npm run test-only && npm run lint && npm run check-types",
    "test-only": "jest",
    "test-coverage": "jest --coverage"
  },
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.4.0",
    "@lukeed/uuid": "^1.0.1",
    "cron": "^1.8.2",
    "discord.js": "^12.3.1",
    "dotenv": "^8.2.0",
    "emoji-regex": "^9.2.0",
    "got": "^11.8.1",
    "ffmpeg": "0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "make-promises-safe": "^5.1.0",
    "redis": "^3.0.2",
    "pino": "^6.7.0",
    "ytdl-core": "^4.4.5"
  },
  "devDependencies": {
    "@types/cron": "^1.7.2",
    "@types/emoji-regex": "^8.0.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "@types/pino": "^6.3.3",
    "@types/redis": "^2.8.28",
    "@types/ws": "^7.2.7",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "jest": "^26.5.3",
    "nodemon": "^2.0.5",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": "14.x"
  }
}
